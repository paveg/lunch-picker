# Copy this file to wrangler.toml.local and replace the IDs with your Cloudflare resources.
# The .local file stays untracked so production credentials never leave your machine.

[[d1_databases]]
binding = "DB"
database_name = "lunch-picker"
database_id = "00000000-0000-0000-0000-000000000000"

[[kv_namespaces]]
binding = "CACHE"
id = "00000000000000000000000000000000"
